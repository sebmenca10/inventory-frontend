{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/api/*": [
        "src/api/*"
      ],
      "@/store/*": [
        "src/store/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/routes/*": [
        "src/routes/*"
      ],
      "@/i18n/*": [
        "src/i18n/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ]
    }
  },
  "types": [
    "vite/client"
  ],
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "include": [
    "src"
  ]
}